[local]

[local:vars]
ansible_ssh_user=vagrant

[local:children]
web_servers
postgres_servers
proxy_servers

[proxy_servers]
proxy ansible_ssh_host=127.0.0.1 ansible_connection=local ansible_python_interpreter=python3

[web_servers]
web ansible_ssh_host=127.0.0.1 ansible_connection=local ansible_python_interpreter=python3

[postgres_servers]
db ansible_ssh_host=127.0.0.1 ansible_connection=local ansible_python_interpreter=python3
